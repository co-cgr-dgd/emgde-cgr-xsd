<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:simpleType name="TS_TipoAcceso">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="NCB3"/>
			<xsd:enumeration value="NCM2"/>
			<xsd:enumeration value="NCA1"/>
			<xsd:enumeration value="NC4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:annotation>
		<xsd:documentation xml:lang="es">
			- NCB3 - PUBLICA.
			- NCM2 - CLASIFICADA.
			- NCA1 - RESERVADA.
			- NC4 - NO CLASIFICADA.
		</xsd:documentation>
	</xsd:annotation>
</xsd:schema>